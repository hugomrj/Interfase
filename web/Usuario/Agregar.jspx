<jsp:root version="2.0"
        xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:h="http://java.sun.com/jsf/html">

    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=2.0" />

    <jsp:directive.page contentType="text/html" pageEncoding="UTF-8"/>
    <jsp:directive.include file="../WEB-INF/jspf/sessiones.jspf"/>

        <style type="text/css" >
            @import "../css/Basic.css" ;
            @import "../css/Media.css" ;
            @import "../css/${SessionTheme}";
        </style>

        <script type="text/javascript">
            <jsp:include page="../js/ajax/libreria.js" />
            <jsp:include page="../js/utilidad/funciones.js"/>
                
            <jsp:include page="../js/validate/Usuario/common.js" />
            <jsp:include page="../js/validate/Usuario/Agregar.js" />
        </script>


    <jsp:element name="html">
        <jsp:attribute name="lang">ES</jsp:attribute>
        <jsp:body>

            <header>
                <div></div>
            </header>

            <nav>
                <jsp:directive.include file="../WEB-INF/jspf/nav.jspf"/>
            </nav>

            <div id="wrapper">

                <aside>
                    <div></div>
                </aside>

                <section>

                    <article>
                            <h3>Agregar Usuario</h3>
                    </article>

                    <article>

                        <form action="../Usuario/Agregar.do"
                            method="post"
                            id="ua_form"
                            name='ua_form'
                            >

                            <input  TYPE="hidden"
                                    name="usuario"
                                    id = "ua_usuario"
                                    value="0"/>

                            <div class = "SinEnfoque"
                                 id="i2">
                                <label for="cuenta">Cuenta:</label>
                                <div class ="textbox">
                                    <input TYPE="TEXT"
                                           name="cuenta"
                                           id = "ua_cuenta"
                                           />
                                </div>
                            </div>

                            <div class= "SinEnfoque"
                                 id="i3">
                                <label for="pass">Contrase√±a:</label>
                                <div class ="textbox">
                                    <input TYPE="PASSWORD"
                                           name="pass"
                                           id = "ua_pass"
                                           />
                                </div>
                            </div>

                        </form>

                    </article>


                    <article class = "botones-footer">

                        <div class ="commandbutton">
                            <a href="javascript:void(0);"
                            class="botonEnlace"
                            id="ua_guardar"
                            >Aceptar</a>
                        </div>


                        <div class ="commandbutton">
                                <a href="javascript:void(0);"
                                class="botonEnlace"
                                id="ua_cancelar"
                                >Cancelar</a>
                        </div>

                      </article>


                </section>
            </div>

            <footer>
                <h6>
                    <jsp:directive.include file="../WEB-INF/jspf/footer.jspf"/>
                </h6>
            </footer>

        </jsp:body>
    </jsp:element>

</jsp:root>
